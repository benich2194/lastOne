<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="playerMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.playerMenuController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addPlayer" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddPlayer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Player" />
      <Button fx:id="removePlayer" layoutX="285.0" layoutY="271.0" mnemonicParsing="false" onAction="#goToRemovePlayer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Player" />
      <Button fx:id="modPlayer" layoutX="285.0" layoutY="179.0" mnemonicParsing="false" onAction="#goToModifyPlayer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Player" />
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
