            <TableColumn prefWidth="70.0" text="Birthdate" />
            <TableColumn prefWidth="84.0" text="Start Work" />
            <TableColumn prefWidth="63.0" text="Level" />
            <TableColumn minWidth="0.0" prefWidth="51.0" text="City" />
            <TableColumn prefWidth="66.0" text="Street" />
            <TableColumn prefWidth="81.0" text="Phone" />
            <TableColumn prefWidth="94.0" text="Right Leg Kicker" />
            <TableColumn prefWidth="75.0" text="Value" />
