         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="playerTableView" editable="true" layoutX="24.0" layoutY="76.0" prefHeight="379.0" prefWidth="658.0" style="-fx-opacity: 0.9;">
