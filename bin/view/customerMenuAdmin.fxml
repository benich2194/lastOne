<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="customerMenuAdmin" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.customerMenuAdminController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addCustomer" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddCustomer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Customer" />
      <Button fx:id="removeCustomer" layoutX="285.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToRemoveCustomer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Customer" />
      <Button fx:id="modCustomer" layoutX="285.0" layoutY="180.0" mnemonicParsing="false" onAction="#goToModifyCustomer" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Customer" />
   </children>
</AnchorPane>
