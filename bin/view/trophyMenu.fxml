<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="trophyMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.172" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.trophyMenuController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addTrophy" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddTro" prefHeight="54.0" prefWidth="131.0" text="Add Trophy" />
      <Button fx:id="removeTrophy" layoutX="285.0" layoutY="387.0" mnemonicParsing="false" onAction="#goToRemoveTrophy" prefHeight="54.0" prefWidth="131.0" text="Remove Trophy" />
      <Button fx:id="modTrophy" layoutX="285.0" layoutY="227.0" mnemonicParsing="false" onAction="#goToModifyTro" prefHeight="54.0" prefWidth="131.0" text="Modify Trophy" />
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
