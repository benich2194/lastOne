<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyStadium" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.modifyStadiumController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="stadiumTableView" editable="true" layoutX="47.0" layoutY="75.0" prefHeight="362.0" prefWidth="618.0" style="-fx-opacity: 0.9;">
        <columns>
          <TableColumn fx:id="stadiumID" editable="false" prefWidth="75.0" text="Stadium ID" />
          <TableColumn fx:id="stadiumName" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="stadiumCapacity" prefWidth="75.0" text="Capacity" />
            <TableColumn fx:id="stadiumCity" prefWidth="75.0" text="City" />
            <TableColumn fx:id="stadiumStreet" prefWidth="75.0" text="Street" />
            <TableColumn fx:id="stadiumHouseNum" prefWidth="75.0" text="House Number" />
            <TableColumn fx:id="stadiumPhoneNum" prefWidth="75.0" text="Phone Number" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
