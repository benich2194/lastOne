<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="coachModif" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CoachModifController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addCoach" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddCoach" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Coach" />
      <Button fx:id="removeCoachButton" layoutX="285.0" layoutY="285.0" mnemonicParsing="false" onAction="#goToRemoveCoach" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Coach" />
      <Button fx:id="modCoach" layoutX="285.0" layoutY="186.0" mnemonicParsing="false" onAction="#goToModifyCoach" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Coach" />
   </children>
</AnchorPane>
