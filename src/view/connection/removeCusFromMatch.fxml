<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="subToCustomer" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.connection.controller.removeCusFromMatchController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ListView fx:id="customerList" layoutX="24.0" layoutY="98.0" onKeyReleased="#updateMatchListtwo" onMouseClicked="#updateMatchList" prefHeight="362.0" prefWidth="322.0" style="-fx-opacity: 0.75;" />
      <ListView fx:id="matchList" layoutX="355.0" layoutY="98.0" prefHeight="362.0" prefWidth="333.0" style="-fx-opacity: 0.75;" />
      <Button layoutX="309.0" layoutY="528.0" mnemonicParsing="false" onAction="#removeCustomerFromMatch" prefHeight="41.0" prefWidth="84.0" text="Remove" />
      <Label layoutX="175.0" layoutY="43.0" prefHeight="41.0" prefWidth="345.0" style="-fx-opacity: 0.75;" text="Remove Customer From Match" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" alignment="CENTER" layoutX="25.0" layoutY="485.0" prefHeight="33.0" prefWidth="662.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
