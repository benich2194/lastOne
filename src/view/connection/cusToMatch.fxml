<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="cusToMatch" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.connection.controller.cusToMatchController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Button fx:id="connectThem" layoutX="309.0" layoutY="528.0" mnemonicParsing="false" onAction="#addCustomerToMatch" prefHeight="41.0" prefWidth="84.0" text="Connect" />
      <Label layoutX="236.0" layoutY="43.0" prefHeight="41.0" prefWidth="217.0" style="-fx-opacity: 0.75;" text="Customer To Match" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" alignment="CENTER" layoutX="25.0" layoutY="485.0" prefHeight="33.0" prefWidth="662.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="customerList" layoutX="83.0" layoutY="113.0" prefHeight="25.0" prefWidth="524.0" promptText="Choose customer:" />
      <ComboBox fx:id="matchList" layoutX="87.0" layoutY="209.0" prefHeight="25.0" prefWidth="524.0" promptText="Choose Match:" />
   </children>
</AnchorPane>
