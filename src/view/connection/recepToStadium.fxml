<AnchorPane fx:id="recepToStadium" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.connection.controller.recepToStadiumController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ListView fx:id="recepList" layoutX="13.0" layoutY="98.0" prefHeight="346.0" prefWidth="333.0" />
      <ListView fx:id="stadiumList" layoutX="350.0" layoutY="98.0" prefHeight="346.0" prefWidth="339.0" />
      <Button fx:id="connectThem" layoutX="315.0" layoutY="510.0" mnemonicParsing="false" onAction="#addRecepToStadium" prefHeight="41.0" prefWidth="84.0" text="Connect" />
      <Label layoutX="221.0" layoutY="41.0" prefHeight="41.0" prefWidth="271.0" style="-fx-opacity: 0.75;" text="Receptionist To Stadium" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" layoutX="29.0" layoutY="468.0" prefHeight="33.0" prefWidth="643.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
