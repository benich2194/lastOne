<AnchorPane fx:id="playerToFirstTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.connection.controller.playerToFirstTeamController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ListView fx:id="playerList" layoutX="11.0" layoutY="98.0" prefHeight="368.0" prefWidth="335.0" />
      <ListView fx:id="teamList" layoutX="350.0" layoutY="98.0" prefHeight="368.0" prefWidth="342.0" />
      <Button fx:id="connectThem" layoutX="314.0" layoutY="494.0" mnemonicParsing="false" onAction="#addPlayerToFirstTeam" prefHeight="41.0" prefWidth="84.0" text="Connect" />
      <Label layoutX="189.0" layoutY="50.0" prefHeight="27.0" prefWidth="396.0" style="-fx-opacity: 0.8;" text="Connect Player To First Team Players" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
