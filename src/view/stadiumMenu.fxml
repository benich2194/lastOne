<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="stadiumMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.stadiumMenuController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.15;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addStadium" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddStadium" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Stadium" />
      <Button fx:id="removeStadium" layoutX="285.0" layoutY="273.0" mnemonicParsing="false" onAction="#goToRemoveStadium" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Stadium" />
      <Button fx:id="modStadium" layoutX="285.0" layoutY="181.0" mnemonicParsing="false" onAction="#goToModifyStadium" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Stadium" />
   </children>
</AnchorPane>
