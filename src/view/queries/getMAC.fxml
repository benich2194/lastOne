<AnchorPane fx:id="addTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.queries.controller.getMACController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" layoutX="-13.0" layoutY="-6.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="70.0" layoutY="106.0" prefHeight="20.0" prefWidth="216.0" style="-fx-opacity: 0.8;" text="Please choose a stadium:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label layoutX="175.0" layoutY="33.0" prefHeight="33.0" prefWidth="349.0" style="-fx-opacity: 0.8;" text="Get Most Active City in a Stadium" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBoxChooseStadium" layoutX="276.0" layoutY="103.0" prefHeight="26.0" prefWidth="323.0" />
      <Button fx:id="buttonGETMAC" layoutX="263.0" layoutY="165.0" mnemonicParsing="false" onAction="#goToShowMAC" prefHeight="33.0" prefWidth="149.0" text="Get City" />
      <TextField fx:id="textFieldMAC" editable="false" layoutX="87.0" layoutY="263.0" prefHeight="26.0" prefWidth="520.0" />
      <Label layoutX="222.0" layoutY="237.0" prefHeight="20.0" prefWidth="323.0" style="-fx-opacity: 0.76;" text="the Most Active city in chosen stadium is:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
