<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="subToCustomer" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.cusToMatchController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ListView fx:id="customerList" blendMode="SCREEN" layoutX="-4.0" layoutY="98.0" prefHeight="93.0" prefWidth="350.0" />
      <Button fx:id="connectThem" layoutX="304.0" layoutY="375.0" mnemonicParsing="false" onAction="#addCustomerToMatch" prefHeight="41.0" prefWidth="84.0" text="Connect" />
      <ListView fx:id="matchList" layoutX="355.0" layoutY="98.0" prefHeight="93.0" prefWidth="350.0" />
      <Label layoutX="256.0" layoutY="25.0" prefHeight="41.0" prefWidth="189.0" text="Customer To Match" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
