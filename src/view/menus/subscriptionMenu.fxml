<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="subscriptionMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.controller.subscriptionMenuController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addSub" layoutX="277.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddSub" prefHeight="54.0" prefWidth="155.0" stylesheets="@Design.css" text="Add Subscription" />
      <Button fx:id="removeSub" layoutX="276.0" layoutY="345.0" mnemonicParsing="false" onAction="#goToRemoveSub" prefHeight="54.0" prefWidth="155.0" stylesheets="@Design.css" text="Remove Subscription" />
      <Button fx:id="modSub" layoutX="277.0" layoutY="179.0" mnemonicParsing="false" onAction="#goToModSub" prefHeight="54.0" prefWidth="155.0" stylesheets="@Design.css" text="Modify Subscription" />
      <Label layoutX="257.0" layoutY="38.0" prefHeight="35.0" prefWidth="209.0" style="-fx-opacity: 0.7;" text="Subscription Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button fx:id="viewSub" layoutX="277.0" layoutY="262.0" mnemonicParsing="false" onAction="#goToViewSub" prefHeight="54.0" prefWidth="155.0" stylesheets="@Design.css" text="View All Subscriptions" />
   </children>
</AnchorPane>
