<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="trophyMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.controller.trophyMenuController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addTrophy" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddTro" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Trophy" />
      <Button fx:id="removeTrophy" layoutX="285.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToRemoveTrophy" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Trophy" />
      <Button fx:id="modTrophy" layoutX="285.0" layoutY="181.0" mnemonicParsing="false" onAction="#goToModifyTro" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Trophy" />
      <Label layoutX="284.0" layoutY="38.0" style="-fx-opacity: 0.7;" text="Trophy Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
