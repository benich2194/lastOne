<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="teamMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.controller.teamMenuController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addTeam" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddTeam" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Add Team" />
      <Button fx:id="removeTeamButton" layoutX="285.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToRemoveTeam" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Remove Team" />
      <Button fx:id="modTeam" layoutX="285.0" layoutY="181.0" mnemonicParsing="false" onAction="#goToModifyTeam" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="Modify Team" />
      <Label layoutX="292.0" layoutY="41.0" style="-fx-opacity: 0.7;" text="Team Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Button fx:id="viewTeams" layoutX="285.0" layoutY="372.0" mnemonicParsing="false" onAction="#goToViewTeams" prefHeight="54.0" prefWidth="131.0" stylesheets="@Design.css" text="View Teams" />
   </children>
</AnchorPane>
