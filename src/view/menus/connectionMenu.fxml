<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="connectionMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.controller.connectionMenuController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="coachTeam" layoutX="247.0" layoutY="143.0" mnemonicParsing="false" onAction="#addCoachToTeam" prefHeight="43.0" prefWidth="177.0" stylesheets="@Design.css" text="Coach to Team" />
      <Button fx:id="playerMenuButton" blendMode="SRC_ATOP" layoutX="247.0" layoutY="257.0" mnemonicParsing="false" onAction="#addReceptionistToStadium" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="Receptionist To Stadium" />
      <Button fx:id="playerFirst" layoutX="244.0" layoutY="200.0" mnemonicParsing="false" onAction="#addPlayerToFirstTeamPlayers" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="Player to First Team Players" />
      <Button fx:id="playerTeam" layoutX="242.0" layoutY="81.0" mnemonicParsing="false" onAction="#addPlayerToTeam" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="Player to Team" />
      <Button fx:id="addMatch" layoutX="247.0" layoutY="378.0" mnemonicParsing="false" onAction="#addMatch" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="add Match" />
      <Button fx:id="cusMatch" layoutX="247.0" layoutY="316.0" mnemonicParsing="false" onAction="#addCustomerToMatch" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="Customer to Match" />
      <Label layoutX="228.0" layoutY="27.0" prefHeight="43.0" prefWidth="229.0" style="-fx-opacity: 0.7;" text="Connection Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <Button layoutX="244.0" layoutY="438.0" mnemonicParsing="false" onAction="#goToRemoveCusFromMatch" prefHeight="43.0" prefWidth="184.0" stylesheets="@Design.css" text="Remove Customer From Match" />
   </children>
</AnchorPane>
