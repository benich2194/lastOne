<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="connectionMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.menus.controller.connectionMenuController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="coachTeam" layoutX="224.0" layoutY="147.0" mnemonicParsing="false" onAction="#addCoachToTeam" prefHeight="43.0" prefWidth="229.0" stylesheets="@Design.css" text="Coach to Team" />
      <Button fx:id="playerMenuButton" blendMode="SRC_ATOP" layoutX="224.0" layoutY="276.0" mnemonicParsing="false" onAction="#addReceptionistToStadium" prefHeight="43.0" prefWidth="229.0" stylesheets="@Design.css" text="Receptionist To Stadium" />
      <Button fx:id="playerFirst" layoutX="224.0" layoutY="213.0" mnemonicParsing="false" onAction="#addPlayerToFirstTeamPlayers" prefHeight="43.0" prefWidth="229.0" stylesheets="@Design.css" text="Player to First Team " />
      <Button fx:id="playerTeam" layoutX="225.0" layoutY="81.0" mnemonicParsing="false" onAction="#addPlayerToTeam" prefHeight="43.0" prefWidth="229.0" stylesheets="@Design.css" text="Player to Team" />
      <Button fx:id="cusMatch" layoutX="224.0" layoutY="338.0" mnemonicParsing="false" onAction="#addCustomerToMatch" prefHeight="43.0" prefWidth="229.0" stylesheets="@Design.css" text=" Customer to Match" />
      <Label layoutX="232.0" layoutY="26.0" prefHeight="43.0" prefWidth="229.0" style="-fx-opacity: 0.7;" text="Connection Menu" textFill="WHITE">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
