<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyPlayer" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.view.controller.viewPlayerController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="playerTableView" editable="true" layoutX="6.0" layoutY="92.0" prefHeight="421.0" prefWidth="689.0" style="-fx-opacity: 0.9;">
        <columns>
          <TableColumn fx:id="playerID" editable="false" prefWidth="57.0" text="ID" />
          <TableColumn fx:id="playerFN" prefWidth="71.0" text="First Name" />
            <TableColumn fx:id="playerLN" prefWidth="16.0" text="Last Name" />
            <TableColumn fx:id="playerBday" editable="false" prefWidth="29.0" text="Birthdate" />
            <TableColumn fx:id="playerSW" editable="false" prefWidth="75.0" text="Start Work" />
            <TableColumn fx:id="playerLvl" prefWidth="79.0" text="Level" />
            <TableColumn fx:id="playerCity" minWidth="0.0" prefWidth="88.0" text="City" />
            <TableColumn fx:id="playerPhoneNum" minWidth="0.0" prefWidth="88.0" text="Phone Number" />
            <TableColumn fx:id="playerRLK" prefWidth="37.0" text="Right Leg Kicker" />
            <TableColumn fx:id="playerVal" prefWidth="211.0" text="Value" />
            <TableColumn fx:id="playerPosition" prefWidth="75.0" text="Position" />
            <TableColumn fx:id="playerCT" prefWidth="75.0" text="Current Team" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="283.0" layoutY="50.0" style="-fx-opacity: 0.8;" text="View Players" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
