<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyMatch" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.modify.controller.modifyMatchController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="279.0" layoutY="37.0" prefHeight="49.0" prefWidth="191.0" style="-fx-opacity: 0.8;" text="Modify Match" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="136.0" layoutY="100.0" style="-fx-opacity: 0.8;" text="Please select a Match:">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBoxMatches" layoutX="288.0" layoutY="95.0" onAction="#showSP" prefHeight="26.0" prefWidth="321.0" />
      <DatePicker fx:id="datepicker" layoutX="256.0" layoutY="181.0" promptText="Select new Date" />
      <Button fx:id="buttonDate" layoutX="259.0" layoutY="226.0" mnemonicParsing="false" onAction="#changeDate" prefHeight="26.0" prefWidth="184.0" text="Modify Date" />
      <TextField fx:id="timepicker" layoutX="256.0" layoutY="275.0" prefHeight="26.0" prefWidth="190.0" promptText="Set Extra Time" />
      <Button fx:id="buttonTime" layoutX="259.0" layoutY="326.0" mnemonicParsing="false" onAction="#changeTime" prefHeight="26.0" prefWidth="184.0" text="Modify Time" />
   </children>
</AnchorPane>
