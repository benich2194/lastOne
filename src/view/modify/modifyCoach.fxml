<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyCoach" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.modify.controller.modifyCoachController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="coachTableView" editable="true" layoutX="27.0" layoutY="85.0" prefHeight="414.0" prefWidth="650.0" style="-fx-opacity: 0.71;">
        <columns>
          <TableColumn fx:id="coachID" editable="false" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="coachFN" prefWidth="75.0" text="First Name" />
            <TableColumn fx:id="coachLN" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="coachBday" editable="false" prefWidth="75.0" text="Birthdate" />
            <TableColumn fx:id="coachSW" editable="false" prefWidth="75.0" text="Start Work Date" />
            <TableColumn fx:id="coachLvl" prefWidth="75.0" text="Level" />
            <TableColumn fx:id="coachHouseNum" prefWidth="75.0" text="House Number" />
            <TableColumn fx:id="coachStreet" prefWidth="75.0" text="Street" />
            <TableColumn fx:id="coachCity" prefWidth="75.0" text="City" />
            <TableColumn fx:id="coachPhoneNum" prefWidth="75.0" text="Phone Number" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label layoutX="272.0" layoutY="35.0" prefHeight="33.0" prefWidth="205.0" text="Modify Coach" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
