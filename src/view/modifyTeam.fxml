<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.modifyTeamController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <TableView fx:id="tableViewTeam" editable="true" layoutX="80.0" layoutY="75.0" prefHeight="346.0" prefWidth="530.0">
        <columns>
          <TableColumn fx:id="teamID" editable="false" prefWidth="81.0" text="Team ID" />
          <TableColumn fx:id="teamName" prefWidth="125.0" text="Name" />
            <TableColumn fx:id="teamValue" prefWidth="88.0" text="Value" />
            <TableColumn fx:id="teamLvl" prefWidth="110.0" text="Level" />
            <TableColumn fx:id="teamStadiumID" prefWidth="125.0" text="Stadium ID" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</AnchorPane>
