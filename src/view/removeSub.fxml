<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="removeSub" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.removeSubController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ComboBox fx:id="subList" layoutX="164.0" layoutY="94.0" prefHeight="25.0" prefWidth="367.0" promptText="Select a subscription to remove:" />
      <Button fx:id="removeButton" layoutX="269.0" layoutY="366.0" mnemonicParsing="false" onAction="#removeSubscription" prefHeight="50.0" prefWidth="152.0" text="Remove Subscription" />
   </children>
</AnchorPane>
