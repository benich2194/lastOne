<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="modifyStadium" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.transfer.controller.transferCoachToTeamController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" layoutY="-22.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="203.0" layoutY="24.0" style="-fx-opacity: 0.8;" text="Transfer Coach To Team" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
      <Label layoutX="24.0" layoutY="77.0" style="-fx-opacity: 0.8;" text="Please double Click a Coach to see teams:">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Label>
      <ListView fx:id="sysTeams" layoutX="18.0" layoutY="308.0" opacity="0.7" prefHeight="127.0" prefWidth="659.0" style="-fx-opacity: 0.8;" />
      <ListView fx:id="coachList" layoutX="21.0" layoutY="101.0" onMouseClicked="#showTeams" opacity="0.7" prefHeight="168.0" prefWidth="659.0" style="-fx-opacity: 0.8;" />
      <Label layoutX="24.0" layoutY="284.0" style="-fx-opacity: 0.8;" text="Select a Team:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="buttonTransfer" layoutX="262.0" layoutY="484.0" mnemonicParsing="false" onAction="#transferThem" prefHeight="42.0" prefWidth="183.0" text="Transfer Coach To Team" />
   </children>
</AnchorPane>
