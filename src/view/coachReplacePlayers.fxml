<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="coachToTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.coachReplacePlayersController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.3;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <ListView fx:id="playerList" blendMode="SCREEN" layoutX="11.0" layoutY="98.0" prefHeight="368.0" prefWidth="335.0" />
      <ListView fx:id="firstTeamList" layoutX="359.0" layoutY="98.0" prefHeight="368.0" prefWidth="326.0" />
      <Button fx:id="replaceButton" layoutX="329.0" layoutY="509.0" mnemonicParsing="false" onAction="#replacePlayers" prefHeight="41.0" prefWidth="84.0" text="Replace" />
      <Label layoutX="138.0" layoutY="16.0" prefHeight="41.0" prefWidth="443.0" text="Replace bench player and first team player" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label layoutX="99.0" layoutY="57.0" prefHeight="41.0" prefWidth="147.0" text="Bench Players" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label layoutX="438.0" layoutY="57.0" prefHeight="41.0" prefWidth="147.0" text="First Players" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
