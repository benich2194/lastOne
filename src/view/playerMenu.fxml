<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="playerMenu" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.playerMenuController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="addPlayer" layoutX="285.0" layoutY="88.0" mnemonicParsing="false" onAction="#goToAddPlayer" prefHeight="54.0" prefWidth="131.0" text="Add Player" />
      <Button fx:id="removePlayer" layoutX="285.0" layoutY="387.0" mnemonicParsing="false" onAction="#goToRemovePlayer" prefHeight="54.0" prefWidth="131.0" text="Remove Player" />
      <Button fx:id="modPlayer" layoutX="285.0" layoutY="227.0" mnemonicParsing="false" onAction="#goToModifyPlayer" prefHeight="54.0" prefWidth="131.0" text="Modify Player" />
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
