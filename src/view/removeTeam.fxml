<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="removeTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.removeTeamController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Label layoutX="201.0" layoutY="43.0" prefHeight="30.0" prefWidth="300.0" text="Please select a team to remove:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="teamList" layoutX="188.0" layoutY="130.0" prefHeight="25.0" prefWidth="331.0" promptText="Existing teams:" />
      <Button fx:id="removeButton" layoutX="291.0" layoutY="436.0" mnemonicParsing="false" onAction="#removeTeam" prefHeight="62.0" prefWidth="120.0" text="Remove" />
   </children>
</AnchorPane>
