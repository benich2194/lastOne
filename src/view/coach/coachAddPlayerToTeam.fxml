<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="playerToTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.coach.controller.coachAddPlayerToTeamController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <ListView fx:id="playerList" blendMode="SCREEN" layoutX="15.0" layoutY="113.0" prefHeight="341.0" prefWidth="671.0" />
      <Button fx:id="connectThem" layoutX="309.0" layoutY="526.0" mnemonicParsing="false" onAction="#addPlayerToTeam" prefHeight="41.0" prefWidth="84.0" text="Add" />
      <Label layoutX="228.0" layoutY="50.0" prefHeight="27.0" prefWidth="261.0" style="-fx-opacity: 0.8;" text="Add Player To My Team" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="firstList" layoutX="272.0" layoutY="478.0" prefWidth="150.0" promptText="First Team?" />
      <Label fx:id="lblMessage" layoutX="146.0" layoutY="84.0" prefHeight="18.0" prefWidth="438.0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
