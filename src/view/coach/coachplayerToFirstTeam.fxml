<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="playerToFirstTeam" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.coach.controller.coachplayerToFirstTeamController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../images/Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ListView fx:id="playerList" layoutX="25.0" layoutY="86.0" prefHeight="362.0" prefWidth="662.0" style="-fx-opacity: 0.76;" />
      <Button fx:id="connectThem" layoutX="314.0" layoutY="512.0" mnemonicParsing="false" onAction="#addPlayerToFirstTeam" prefHeight="41.0" prefWidth="84.0" text="Connect" />
      <Label layoutX="190.0" layoutY="34.0" prefHeight="27.0" prefWidth="396.0" style="-fx-opacity: 0.8;" text="Connect Player To First Team Players" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label fx:id="lblMessage" layoutX="180.0" layoutY="65.0" prefHeight="18.0" prefWidth="368.0" textFill="#268594">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" layoutX="35.0" layoutY="467.0" prefHeight="33.0" prefWidth="643.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
