<AnchorPane fx:id="removeSub" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.remove.controller.removeSubController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ComboBox fx:id="subList" layoutX="162.0" layoutY="107.0" prefHeight="25.0" prefWidth="367.0" promptText="Select a subscription to remove:" />
      <Button fx:id="removeButton" layoutX="269.0" layoutY="366.0" mnemonicParsing="false" onAction="#removeSubscription" prefHeight="44.0" prefWidth="152.0" text="Remove Subscription" />
      <Label layoutX="236.0" layoutY="50.0" style="-fx-opacity: 0.8;" text="Remove Subscription" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
