<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="removeTrophy" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.remove.controller.removeTrophyController">
   <children>
      <ImageView fitHeight="590.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ComboBox fx:id="troList" layoutX="151.0" layoutY="89.0" prefHeight="33.0" prefWidth="400.0" promptText="Select trophy to remove:" />
      <Button fx:id="removeButton" layoutX="265.0" layoutY="394.0" mnemonicParsing="false" onAction="#removeTrophy" prefHeight="43.0" prefWidth="122.0" text="Remove Trophy" />
      <Label layoutX="235.0" layoutY="50.0" style="-fx-opacity: 0.8;" text="Remove Trophy" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" layoutX="14.0" layoutY="331.0" prefHeight="37.0" prefWidth="689.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
