<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="removeMatch" prefHeight="550.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.remove.controller.removeMatchController">
   <children>
      <ImageView fitHeight="550.0" fitWidth="701.0" style="-fx-opacity: 0.2;">
         <image>
            <Image url="@../../Soccer-Stadium.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <ComboBox fx:id="matchList" layoutX="148.0" layoutY="111.0" prefHeight="25.0" prefWidth="439.0" promptText="Please select a match to remove:" />
      <Button fx:id="removeButton" layoutX="278.0" layoutY="261.0" mnemonicParsing="false" onAction="#removeMatch" prefHeight="44.0" prefWidth="168.0" text="Remove Match" />
      <Label layoutX="274.0" layoutY="50.0" prefHeight="44.0" prefWidth="175.0" text="Remove Match" textFill="WHITE">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <Label fx:id="labelSuccess" layoutX="168.0" layoutY="209.0" prefHeight="33.0" prefWidth="399.0" textFill="#017c87">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
